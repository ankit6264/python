print(a,"*",a2,"=",a*a2)
print(a,"*",a3,"=",a*a3)
print(a,"*",a4,"=",a*a4)
print(a,"*",a5,"=",a*a5)
print(a,"*",a6,"=",a*a6)
print(a,"*",a7,"=",a*a7)
print(a,"*",a8,"=",a*a8)
print(a,"*",a9,"=",a*a9)
